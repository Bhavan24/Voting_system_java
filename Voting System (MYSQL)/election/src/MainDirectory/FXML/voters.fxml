<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="635.0" maxWidth="855.0" prefHeight="635.0" prefWidth="855.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainDirectory.Controllers.VoterController">
    <children>
      <Group layoutX="43.0" layoutY="30.0">
         <children>
              <Button mnemonicParsing="false" onAction="#importVoter" prefHeight="50.0" prefWidth="50.0" styleClass="importBtn" stylesheets="@../CSS/stylesheet.css">
               <tooltip>
                  <Tooltip text="Import Voter" />
               </tooltip></Button>
            <Button layoutX="80.0" mnemonicParsing="false" onAction="#addVoter" prefHeight="50.0" prefWidth="50.0" styleClass="addBtn" stylesheets="@../CSS/stylesheet.css">
               <tooltip>
                  <Tooltip text="Add Voter" />
               </tooltip></Button>
            <Button layoutX="160.0" mnemonicParsing="false" onAction="#editVoter" prefHeight="50.0" prefWidth="50.0" styleClass="editBtn" stylesheets="@../CSS/stylesheet.css">
               <tooltip>
                  <Tooltip text="Edit Voter" />
               </tooltip></Button>
            <Button layoutX="240.0" mnemonicParsing="false" onAction="#deleteVoter" prefHeight="50.0" prefWidth="50.0" styleClass="deleteBtn" stylesheets="@../CSS/stylesheet.css">
               <tooltip>
                  <Tooltip text="Delete Voter" />
               </tooltip></Button>
            <Button layoutX="320.0" mnemonicParsing="false" onAction="#refreshTable" prefHeight="50.0" prefWidth="50.0" styleClass="refresh" stylesheets="@../CSS/stylesheet.css">
               <tooltip>
                  <Tooltip text="Refresh Table" />
               </tooltip></Button>
         </children>
      </Group>
        <TableView fx:id="voterTable" layoutX="42.0" layoutY="98.0" prefHeight="500.0" prefWidth="771.0" stylesheets="@../CSS/stylesheet.css">
            <columns>
                <TableColumn fx:id="idColumnVoter" prefWidth="68.0" style="-fx-alignment: center;" text="VOTER ID" />
                <TableColumn fx:id="fNameColumnVoter" maxWidth="150.0" prefWidth="150.0" style="-fx-alignment: center;" text="FIRST NAME" />
                <TableColumn fx:id="lNameColumnVoter" prefWidth="150.0" style="-fx-alignment: center;" text="LAST NAME" />
            <TableColumn fx:id="genderColumnVoter" maxWidth="154.0" prefWidth="71.0" style="-fx-alignment: center;" text="GENDER" />
            <TableColumn fx:id="dobColumnVoter" maxWidth="207.0" prefWidth="110.0" style="-fx-alignment: center;" text="DATE OF BIRTH" />
                <TableColumn fx:id="addressColumnVoter" prefWidth="221.0" style="-fx-alignment: center;" text="ADDRESS" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
